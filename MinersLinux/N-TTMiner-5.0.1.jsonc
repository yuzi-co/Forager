            "Params": "-coin RVN",
            "Mem": 3
        },
        // "Lyra2v3": "-a Lyra2v3",
        "MTP": {
            "Params": "-a MTP",
            "Mem": 4.5
        },
        "ProgPOWZ": "-coin ZANO",
        "ProgPOWEpic": "-coin EPIC",
        "ProgPOWHora": "-coin HORA",
        "ProgPOWSero": "-coin SERO",
        "Ubqhash": {
            "Params": "-a Ubqhash",
            "Mem": 3
        }
