{
  "Path": "miner",
  "Uri": "https://github.com/develsoftware/GMinerRelease/releases/download/2.09/gminer_2_09_linux64.tar.xz",
  "Type": "NVIDIA",
  "CUDA": 9.0,
  "Arguments": [
    "--server #Server#:#Port#",
    "--user #Login#",
    "--pass #Password#",
    "--api #APIPort#",
    "--watchdog 0",
    "$(if($EnableSSL){'--ssl 1'})",
    "--devices #DevicesETHMode#",
    "--cuda 1",
    "--opencl 0",
    "--color 0",
    "#AlgorithmParameters#",
    "#CustomParameters#"
  ],
  "API": "GMiner",
  "SSL": true,
  "Fee": 0.02,
  "Algorithms": {
    "_BeamV2": "--algo BeamHashII",
    "CnBBC": {
      "Params": "--algo bbc",
      "NoCpu": true
    },
    "Cortex": {
      "Params": "--algo cortex",
      "Fee": 0.05
    },
    "CuckooBFC": {
      "Params": "--algo bfc",
      "NoCpu": true,
      "Fee": 0.03
    },
    "Cuckoo24": {
      "Params": "--algo cuckoo24",
      "NoCpu": true,
      "Fee": 0.03
    },
    "Cuckoo29": {
      "Params": "--algo cuckoo29",
      "NoCpu": true,
      "Mem": 3.8
    },
    "Cuckaroo29": {
      "Params": "--algo cuckaroo29",
      "NoCpu": true,
      "Mem": 3.8
    },
    "Cuckaroo29d": {
      "Params": "--algo cuckarood29",
      "NoCpu": true,
      "Mem": 3.8
    },
    "Cuckaroo29m": {
      "Params": "--algo cuckaroom29",
      "NoCpu": true,
      "Mem": 3.8
    },
    "Cuckaroo29s": {
      "Params": "--algo swap",
      "NoCpu": true,
      "Mem": 3.8
    },
    "Cuckaroo29v": {
      "Params": "--algo cuckarood29v",
      "NoCpu": true,
      "Mem": 3.8
    },
    "Cuckatoo31": {
      "Params": "--algo grin31",
      "NoCpu": true,
      "Mem": 7.4
    },
    "Cuckatoo32": {
      "Params": "--algo grin32",
      "NoCpu": true,
      "Mem": 7.4
    },
    "_Eaglesong": "--algo eaglesong",
    "Equihash96": "--algo 96_5 --pers auto",
    "_Equihash125": "--algo 125_4",
    "_Equihash144": {
      "Params": "--algo 144_5 --pers auto",
      "Mem": 1.75
    },
    "Equihash150": {
      "Params": "--algo 150_5",
      "Mem": 2.9
    },
    "Equihash192": {
      "Params": "--algo 192_7 --pers auto",
      "Mem": 2.75
    },
    "Equihash210": "--algo 210_9 --pers AION0PoW",
    "EquihashVds": "--algo vds",
    "_Ethash": {
      "Params": "--algo ethash --proto stratum",
      "Mem": 3,
      "Fee": 0.0065
    },
    "_Ethash_Eaglesong": {
      "Params": "--algo eth+ckb --proto stratum --dserver #ServerDual#:#PortDual# --duser #LoginDual# --dpass #PasswordDual#",
      "Mem": 3,
      "Fee": 0.03
    },
    "_Ethash_Kadena": {
      "Params": "--algo eth+kda --proto stratum --dserver #ServerDual#:#PortDual# --duser #LoginDual# --dpass #PasswordDual#",
      "Mem": 3,
      "Fee": 0.03
    },
    "_Ethash_Handshake": {
      "Params": "--algo eth+hns --proto stratum --dserver #ServerDual#:#PortDual# --duser #LoginDual# --dpass #PasswordDual#",
      "Mem": 3,
      "NH": false,
      "Fee": 0.03
    },
    "Handshake": {
      "Params": "--algo Handshake",
      "NH": false
    },
    "Kadena": "--algo Blake2s",
    "KawPow": "--algo kawpow",
    "ProgPowSero": "--algo sero"
  }
}
