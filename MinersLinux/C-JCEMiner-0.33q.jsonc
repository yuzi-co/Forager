{
  "Path": "jce_cn_cpu_miner64",
  "Uri": "https://github.com/jceminer/cn_cpu_miner/raw/master/jce_cn_cpu_miner.linux.033q.zip",
  "SHA256": "1901DC7F4D4D39EBFFBF07690B6A334364F34F063AFF14FF4799789941AD9B8F",
  "Type": "CPU",
  "Arguments": [
    "-o #Server#:#Port#",
    "-u #Login#",
    "-p #Password#",
    "$(if ($enableSSL){'--ssl'})",
    "$(if ($Pool.PoolName -eq 'NiceHash'){'--nicehash'})",
    "--low",
    "--any",
    "--auto",
    "--mport #APIPort#",
    "--stakjson",
    "$(if ($CpuThreads -ne $null){'-t ' + $CpuThreads})",
    "#AlgorithmParameters#",
    "#CustomParameters#"
  ],
  "API": "XMRig",
  "Fee": "$(if($SystemInfo.CpuFeatures.aes){0.015}else{0.03})",
  "SSL": true,
  "Algorithms": {
    "CnAlloy": "--variation 8",
    "CnArto": "--variation 10",
    "CnDark": "--variation 17",
    "CnFast": "--variation 11",
    "CnFreeHaven": "--variation 18",
    "CnHalf": "--variation 21",
    // "CnHaven": "--variation 12",
    // "CnHeavy": "--variation 5",
    "CnLiteV7": "--variation 4",
    "CnMkt": "--variation 9",
    "CnRed": "--variation 14",
    // "CnSaber": "--variation 13",
    "CnTurtle": "--variation 20",
    "CnUplexa": "--variation 19",
    "CnWaltz": "--variation 22",
    "CnXTL": "--variation 7"
  }
}
