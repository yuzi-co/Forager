    "YescryptR32": "-a yescryptr32",
    "Yespower": "-a yespower",
    "YespowerIC": "-a yespowerIC",
    "YespowerIOTS": "-a yespowerIOTS",
    "YespowerITC": "-a yespowerITC",
    "YespowerLITB": "-a yespowerLITB",
    "YespowerLTNCG": "-a yespowerLTNCG",
    "YespowerR16": "-a yespowerr16",
