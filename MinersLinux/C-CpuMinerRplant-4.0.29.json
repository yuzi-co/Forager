    "Argon2d4096": "-a argon2d4096",
    "Argon2dGlt": "-a argon2dglt",
    "Argon2iGlt": "-a argon2iglt",
    "Argon2m": "-a argon2m",
    "Binarium": "-a Binarium_hash_v1",
    "CpuPower": "-a CpuPower",
    "Cryptovantaa": "-a cryptovantaa",
    "Lyra2cz": "-a lyra2cz",
    "Lyra2h": "-a lyra2h",
    "Lyra2z330": "-a lyra2z330",
    "Power2b": "-a power2b",
    "SHA256csm": "-a sha256csm",
    "Verium": "-a scrypt:1048576",
    "YescryptR8": "-a yescryptr8",
    "YescryptR8g": "-a yescryptr8g",
    "YescryptR8glt": "-a yescryptr8glt",
    "YescryptR16": "-a yescryptr16",
    "YescryptR16V2": "-a yescryptr16v2",
