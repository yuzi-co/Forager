{
  "Path": "miner",
  "Uri": "https://github.com/develsoftware/GMinerRelease/releases/download/2.09/gminer_2_09_linux64.tar.xz",
  "Type": "AMD",
  "Arguments": [
    "--server #Server#:#Port#",
    "--user #Login#",
    "--pass #Password#",
    "--api #APIPort#",
    "--watchdog 0",
    "$(if($EnableSSL){'--ssl 1'})",
    "--devices #DevicesETHMode#",
    "--cuda 0",
    "--opencl 1",
    "--color 0",
    "#AlgorithmParameters#",
    "#CustomParameters#"
  ],
  "API": "GMiner",
  "SSL": true,
  "Fee": 0.02,
  "Algorithms": {
    "BeamV2": "--algo BeamHashII",
    "CuckooBFC": {
      "Params": "--algo bfc",
      "NoCpu": true,
      "Fee": 0.03
    },
    "Cuckoo24": {
      "Params": "--algo cuckoo24",
      "NoCpu": true,
      "Fee": 0.03
    },
    "Cuckoo29": {
      "Params": "--algo cuckoo29",
      "NoCpu": true,
      "Mem": 3.8
    },
    "Cuckaroo29": {
      "Params": "--algo cuckaroo29",
      "NoCpu": true,
      "Mem": 3.8
    },
    "Cuckaroo29d": {
      "Params": "--algo cuckarood29",
      "NoCpu": true,
      "Mem": 3.8
    },
    "Cuckaroo29m": {
      "Params": "--algo cuckaroom29",
      "NoCpu": true,
      "Mem": 3.8
    },
    "Cuckaroo29s": {
      "Params": "--algo swap",
      "NoCpu": true,
      "Mem": 3.8
    },
    "Cuckaroo29v": {
      "Params": "--algo cuckarood29v",
      "NoCpu": true,
      "Mem": 3.8
    },
    "_Eaglesong": "--algo eaglesong",
    "Equihash125": "--algo 125_4",
    "Equihash144": {
      "Params": "--algo 144_5 --pers auto",
      "Mem": 1.75
    },
    "Equihash150": {
      "Params": "--algo 150_5",
      "Mem": 2.9
    },
    "Equihash192": {
      "Params": "--algo 192_7 --pers auto",
      "Mem": 2.75
    },
    "Kadena": "--algo Blake2s"
  }
}
