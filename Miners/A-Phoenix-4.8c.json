{
  "Path": "PhoenixMiner.exe",
  "Uri": "https://github.com/yuzi-co/miners/releases/download/2019.12/PhoenixMiner_4.8c_Windows.7z",
  "Type": "AMD",
  "Arguments": [
    "-pool #Protocol#://#Server#:#Port#",
    "-wal #Login#",
    "-pass #Password#",
    "-dpool #ProtocolDual#://#ServerDual#:#PortDual#",
    "-dwal #LoginDual#",
    "-dpsw #PasswordDual#",
    "$(if ('#EthStMode#' -ne ''){'-esm #EthStMode#'})",
    "-mi 12",
    "-wd 1",
    "-r -1",
    "-logfile #GroupName#_log.txt",
    "-logsmaxsize 10",
    "-amd",
    "-di #DevicesClayMode#",
    "-cdmport #APIPort#",
    "-cdm 1",
    "#AlgorithmParameters#",
    "#CustomParameters#"
  ],
  "API": "Claymore",
  "Fee": 0.0065,
  "SSL": true,
  "Mem": 3,
  "Algorithms": {
    "ProgPOW": "-coin bci -mode 1",
    "Ethash": "-mode 1",
    "Ubqhash": "-coin ubq -mode 1",
    "_Ethash_Blake2s": "-dcoin blake2s -mode 0"
  }
}
