{
  "Path": "PhoenixMiner.exe",
  "Uri": "https://github.com/yuzi-co/miners/releases/download/2020.06/PhoenixMiner_5.0e_Windows.7z",
  "Type": "NVIDIA",
  "Arguments": [
    "-pool #Protocol#://#Server#:#Port#",
    "-wal #Login#",
    "-pass #Password#",
    "-dpool #ProtocolDual#://#ServerDual#:#PortDual#",
    "-dwal #LoginDual#",
    "-dpsw #PasswordDual#",
    "$(if ('#EthStMode#' -ne ''){'-esm #EthStMode#'})",
    "-mi 12",
    "-wd 1",
    "-r -1",
    "-logfile #GroupName#_log.txt",
    "-logsmaxsize 10",
    "-nvidia",
    "-nvdo 1",
    "-di #DevicesClayMode#",
    "-cdmport #APIPort#",
    "-cdm 1",
    "#AlgorithmParameters#",
    "#CustomParameters#"
  ],
  "API": "Claymore",
  "Fee": 0.0065,
  "SSL": true,
  "Mem": 3,
  "Algorithms": {
    "Ethash": "-mode 1",
    "Ubqhash": "-coin ubq -mode 1"
    // "Ethash_Blake2s": "-dcoin blake2s -mode 0"
  }
}
